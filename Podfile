# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'NiharExpress' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'SwiftyJSON', '~> 4.0'
  
  pod 'CryptoSwift', '~> 1.0'
  
  pod 'Cosmos', '~> 22.1' ## StarRating
  
  # Image caching library
  pod 'PINRemoteImage'
  
  # pull to refresh
  pod "ESPullToRefresh"
  
  # Google Firebase for Notifications
  pod 'Firebase/Analytics'
  
  pod 'Firebase/Messaging'
  
  # Force updated app to get downloaded
  pod 'Siren', :git => 'https://github.com/ArtSabintsev/Siren.git', :branch => 'swift5.0' # Swift 5.0
  
  # Pods for NiharExpress

end
